{
  "name": "chrome-storage-helper",
  "version": "1.0.0",
  "description": "一个chrome-extension，方便在开发、预发、正式环境之间的对登录信息快速切换，并支持跨机器实现用户信息复现。",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env mode=development webpack serve",
    "build": "webpack -c build/webpack.config.ts",
    "eslint": "eslint --ext .js,.ts,.jsx,.tsx . --fix",
    "prettier": "prettier --write .",
    "preinstall": "husky install",
    "commit": "git add . && git-cz"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": []
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.22.15",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/cz-commitlint": "^18.4.3",
    "@types/node": "^20.10.5",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/uuid": "^9.0.7",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "babel-loader": "^9.1.3",
    "classnames": "^2.5.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "inquirer": "8",
    "less-loader": "^11.1.4",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "4.24.12",
    "class-css": "^1.0.24",
    "react": "18",
    "react-dom": "18",
    "uuid": "^9.0.1"
  }
}
